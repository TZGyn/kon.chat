{
	"$schema": "https://openapi.vercel.sh/vercel.json",
	"headers": [
		{
			"source": "/",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=2592000, immutable"
				}
			]
		},
		{
			"source": "/:path*",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=2592000, immutable"
				}
			]
		}
	]
}
