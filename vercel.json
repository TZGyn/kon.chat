{
	"$schema": "https://openapi.vercel.sh/vercel.json",
	"headers": [
		{
			"source": "/service-worker.js",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=0, must-revalidate"
				}
			]
		},
		{
			"source": "/",
			"headers": [
				{ "key": "Cache-Control", "value": "max-age=86400" }
			]
		},
		{
			"source": "/:path*",
			"headers": [
				{ "key": "Cache-Control", "value": "max-age=86400" }
			]
		}
	]
}
