{
	"$schema": "https://openapi.vercel.sh/vercel.json",
	"headers": [
		{
			"source": "/",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=0, must-revalidate"
				}
			]
		},
		{
			"source": "/:path*",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=2592000, immutable"
				}
			]
		}
	],
	"git": {
		"deploymentEnabled": {
			"master": true,
			"merge_backend": false
		}
	}
}
