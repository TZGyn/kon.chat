{
	"$schema": "https://openapi.vercel.sh/vercel.json",
	"headers": [
		{
			"source": "/",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=0, must-revalidate"
				}
			]
		},
		{
			"source": "/:path*",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=2592000, immutable"
				}
			]
		}
	],
	"cleanUrls": true,
	"rewrites": [
		{
			"source": "/documents/pdf/(.*)",
			"destination": "/documents/pdf/1"
		},
		{ "source": "/youtube/(.*)", "destination": "/youtube/1" },
		{ "source": "/chat/(.*)", "destination": "/chat/1" },
		{ "source": "/(.*)", "destination": "/200" }
	],
	"git": {
		"deploymentEnabled": {
			"merge_backend": false
		}
	}
}
